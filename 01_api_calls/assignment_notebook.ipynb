    "import json\n",
    "import pickle\n",
    "response = urllib.request.urlopen(url+query)\n",
    "response_bytes = response.read()\n",
    "data = json.loads(response_bytes)\n",
    "response.close() \n",
    "\n",
    "import pickle\n",
    "with open('json_data.pkl', 'wb') as file:\n",
    "    pickle.dump(data, file)"
